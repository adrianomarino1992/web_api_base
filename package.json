{
  "name": "web_api_base",
  "version": "8.0.0",
  "description": "web api base",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "README.md"
  ],
  "readme": "README.md",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./documentation": {
      "import": "./dist/documentation/Index.js",
      "require": "./dist/documentation/Index.js",
      "types": "./dist/documentation/Index.d.ts"
    },
    "./validation": {
      "import": "./dist/validation/Index.js",
      "require": "./dist/validation/Index.js",
      "types": "./dist/validation/Index.d.ts"
    },
    "./dependencyInjection": {
      "import": "./dist/dependencyInjection/Index.js",
      "require": "./dist/dependencyInjection/Index.js",
      "types": "./dist/dependencyInjection/Index.d.ts"
    },
    "./metadata": {
      "import": "./dist/metadata/Index.js",
      "require": "./dist/metadata/Index.js",
      "types": "./dist/metadata/Index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "documentation": [
        "./dist/documentation/Index.d.ts"
      ],
      "validation": [
        "./dist/validation/Index.d.ts"
      ],
      "dependencyInjection": [
        "./dist/dependencyInjection/Index.d.ts"
      ],
      "metadata": [
        "./dist/metadata/Index.d.ts"
      ]
    }
  },
  "bin": {
    "create-controller": "./dist/bin/CreateController.js",
    "create-application": "./dist/bin/CreateApplication.js"
  },
  "scripts": {
    "test": "jest",
    "debug": "node ./dist/index.js --debug",
    "transpile": "tsc -p .",
    "tests": "npx jest"
  },
  "author": "adriano.marino1992@gmail.com",
  "repository": "https://github.com/adrianomarino1992/web_api_base.git",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "@types/express": "^4.17.15"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.11.17",
    "@types/reflect-metadata": "^0.1.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
